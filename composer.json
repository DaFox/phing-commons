{
    "name": "elnebuloso/phing-commons",
    "description": "phing commons build stack",
    "type": "library",
    "keywords":
        [
            "phing",
            "build management",
            "continuous integration"
        ],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/elnebuloso/phing"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "phing/phing": "dev-master",
        "apigen/apigen": "2.8.0",
        "sebastian/phpcpd": "2.0.0",
        "phpunit/phpunit": "3.7.32",
        "squizlabs/php_codesniffer": "1.5.2",
        "pdepend/pdepend": "1.1.3",
        "phpmd/phpmd": "1.5.0"
    },
    "autoload": {
        "psr-0": {
            "PhingCommons\\": "src/"
        }
    }
}