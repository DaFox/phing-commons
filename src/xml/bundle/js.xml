<?xml version="1.0" encoding="UTF-8"?>


<project name="bundle.js">


    <!-- ============================================ -->
    <!-- bundle.js:before                             -->
    <!-- ============================================ -->
    <target name="bundle.js:before">
        <echo message="no actions defined" />
    </target>


    <!-- ============================================ -->
    <!-- bundle.js:main                               -->
    <!-- ============================================ -->
    <target name="bundle.js:main">
        <phing-commons-js-bundle
                file="${manifest.js}"
                public="${project.public}"
                targetDir="${project.public}"
                compileLevel="${bundle.js.compile.level}"
                compileWarningLevel="${bundle.js.compile.warning.level}" />
    </target>


    <!-- ============================================ -->
    <!-- bundle.js:after                              -->
    <!-- ============================================ -->
    <target name="bundle.js:after">
        <echo message="no actions defined" />
    </target>


    <!-- ============================================ -->
    <!-- bundle.js                                    -->
    <!-- ============================================ -->
    <target name="bundle.js" depends="bundle.js:before, bundle.js:main, bundle.js:after" />


</project>