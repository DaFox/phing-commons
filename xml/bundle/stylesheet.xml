<?xml version="1.0" encoding="UTF-8"?>


<project name="bundle.stylesheet">


    <!-- ============================================ -->
    <!-- bundle.stylesheet:before                     -->
    <!-- ============================================ -->
    <target name="bundle.stylesheet:before">
        <echo message="no actions defined" />
    </target>


    <!-- ============================================ -->
    <!-- bundle.stylesheet:main                       -->
    <!-- ============================================ -->
    <target name="bundle.stylesheet:main">
        <phing-commons-bundle-stylesheet
                target="${project.public.stylesheet}"
                java="${java}"
                compiler="${bundle.stylesheet.compiler}"
                verbose="${bundle.stylesheet.verbose}"
                manifest="${manifest.stylesheet}"
                folder="${project.public}" />
    </target>


    <!-- ============================================ -->
    <!-- bundle.stylesheet:after                      -->
    <!-- ============================================ -->
    <target name="bundle.stylesheet:after">
        <echo message="no actions defined" />
    </target>


    <!-- ============================================ -->
    <!-- bundle.stylesheet                            -->
    <!-- ============================================ -->
    <target name="bundle.stylesheet" depends="bundle.stylesheet:before, bundle.stylesheet:main, bundle.stylesheet:after" />


</project>