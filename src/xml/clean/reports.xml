    <!-- ============================================ -->
    <target name="clean.tmp:before">
        <echo message="no actions defined" />
    </target>


    <!-- ============================================ -->
    <!-- clean.tmp:main                               -->
    <!-- ============================================ -->
    <target name="clean.tmp:main">
        <delete dir="${build.tmp}" />
        <mkdir dir="${build.tmp}" />
    </target>


    <!-- ============================================ -->
    <!-- clean.tmp:after                              -->
    <!-- ============================================ -->
